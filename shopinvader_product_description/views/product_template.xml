<odoo>

    <record model="ir.ui.view" id="product_template_form_view">
        <field
            name="name"
        >product.template.form (in shopinvader_product_description)</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="shopinvader_product.product_template_form_view" />
        <field name="arch" type="xml">
            <group name="description">
                <separator string="Short Description" colspan="4" />
                <field name="short_description" colspan="4" nolabel="1" />
                <separator string="Description" colspan="4" />
                <field name="description" colspan="4" nolabel="1" />
            </group>
        </field>
    </record>

</odoo>
